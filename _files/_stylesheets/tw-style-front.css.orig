    font-size:15px;
}
.form-control {
    border: 1px solid #DEDEDE;
    height:45px;
    border-radius: 0px;
    margin-right: -3px;
}
.head_title {
    margin-bottom: 20px;
}
.head_title p {
    max-width: 75%;
    margin: 10px auto;
    padding-bottom: 20px;
}


/*For Footer Menu Section*/

.footer-menu-link {
  color:var(--darkpurple);
}

.footer-menu{
    background:#f6f6f6;
    color:var(--darkpurple);
    border-bottom:1px solid #cfcdcd;
    grid-column: span 8;
    z-index:2;
    padding:100px 0px;

    /* height: 30vh; */
}
.menu-item-container {
  display:flex;
  justify-content: center;
}
.menu-box {
    border: 1px solid #dddddd;
    padding: 0px;
    background-color: #ffffff;
    margin-right:15px;
    width:30%;
    cursor:pointer;
}
.menu-box:last-child {
  margin-right:0px;
}
.menu-box:hover {
  box-shadow: 0 15px 20px rgba(0,0,0,.1);
  -webkit-transition: all 350ms ease-in-out;
  transition: all 350ms ease-in-out;
}

.menu-item {
  padding: 30px 0px;
}

.menu-item svg {
  height: 60px;
  margin-bottom: 10px;
  width:100%;
  margin-left:auto;
  margin-right:auto;
}

.footer-menu-link:hover {
  box-shadow: 0 15px 20px rgba(0,0,0,.1);
  -webkit-transition: all 350ms ease-in-out;
  transition: all 350ms ease-in-out;
  color:var(--purple) ;
}


.footer-menu-link :hover path.cls-1{
  box-shadow: 0 15px 20px rgba(0,0,0,.1);
  -webkit-transition: fill 350ms ease-in-out;
  transition: fill 350ms ease-in-out;
  fill:var(--purple) ;
}

.footer-menu-link .cls-2 {
    fill:none;
    stroke:#000;
    stroke-linecap:round;
    stroke-linejoin:round;
    stroke-width:2px;

}

.footer-menu-link :hover .cls-2 {
    box-shadow: 0 15px 20px rgba(0,0,0,.1);
    -webkit-transition: fill 350ms ease-in-out;
    transition: fill 350ms ease-in-out;
    stroke:var(--purple) ;
    stroke-width:2px;
    fill:none;
}

.menu-item h5{
  font-size:18px;
  font-family: 'Roboto', sans-serif;
  font-weight:500;
   text-align: center;
   vertical-align: middle;
   margin-bottom:.5rem !important;
}
.menu-item p{
    font-size:14px;
	   text-align: center;
     vertical-align: middle;
     color:#8b96aa;
     transition:none;
}
.menu-item ul li{
    font-size:14px;
    font-family: 'Roboto', sans-serif;
    font-weight: 400;
    margin:10px
}


/* Search results */

.algolia-docsearch-suggestion--highlight {
  font-weight: bold;
}
.modal-body a{
    font-size:14px;
    font-family:"Helvetica Neue",Helvetica,Arial,sans-serif;
    color:#337ab7;
}
.modal-body p{
    font-size:14px;
    font-family:"Helvetica Neue",Helvetica,Arial,sans-serif;
    color:#333;
}


/* MEDIA QUERIES */

@media (min-width: 1000px) and (max-width: 480px) {
    .dropdown-slide .dropdown-menu {
      -webkit-transform: none;
              transform: none;
    }
  }

@media (max-width:700px) {
    .menu-item-container {
        display:flex;
        flex-wrap: wrap;
    }

    .menu-box {
        width: 50%;
        margin:0;
    }
}

  @media (max-width: 480px) {
    .navbar-toggleable-md > .container {
      margin-right: 0;
      margin-left: 0;
      padding-left: 25px;
      padding-right: 25px;
    }

    .menu-box {
        width:100%;
        margin-bottom:20px;
    }
  }
